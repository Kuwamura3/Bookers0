<!--Sign_up-->
<div class="body">
  <div class="container">
    <h2>Log in</h2>
    <%= form_with model: @user, url: session_path(resource_name), local: true do |f| %>
      <div class="field">
        <%= f.label :name %><br />
        <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
      </div>
      <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password, autocomplete: "off", id:"inputPassword", placeholder:"パスワード" %>
      </div>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>

